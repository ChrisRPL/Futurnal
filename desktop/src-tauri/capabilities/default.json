{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Futurnal Desktop Shell",
  "windows": ["main", "command-palette"],
  "permissions": [
    "core:default",
    "store:default",
    "updater:default",
    "dialog:default",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-open",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister",
    "global-shortcut:allow-is-registered"
  ]
}
